{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "Extensão RCS",
    "description": "Extensão RCS",
    "author": "Rogério C Santana",
    "browser_action": {
   	 "default_title": "Extensão RCS",
   	 "default_popup": "index.html",
   	 "default_icon": "icon.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "<all_urls>"
    ],   
    "web_accessible_resources": ["https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"], 
    "background": {
        "scripts": ["jquery-1.9.1.js","rcsBook.js","functions.js"],
        "persistent": false
    }
}