{
    "manifest_version": 2,
    "version": "0.1.2",
    "name": "Atalhos FLUIG",
    "description": "Atalhos para agilizar a pesquisa e acessos no Fluig",
    "author": "Rogerio C Santana",
    "browser_action": {
     "default_title": "Fluig Helper",
     "default_popup": "index.html",
     "default_icon": "icon.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "<all_urls>"
    ],   
    "web_accessible_resources": ["https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"], 
    "background": {
        "scripts": ["jquery-1.9.1.js","rcsBook.js","functions.js"],
        "persistent": false
    }
}